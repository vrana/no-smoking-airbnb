{
	"manifest_version": 2,
	"name": "No smoking Airbnb",
	"description": "Highlights Smoking Allowed places on airbnb.com.",
	"version": "1.0",
	"content_scripts": [
		{
			"matches": ["https://*.airbnb.com/rooms/*"],
			"js": ["nosmoking.js"]
		}
	],
	"icons": {"128": "icon.png"},
	"permissions": ["https://www.airbnb.com/"],
	"web_accessible_resources": ["smoking.svg"]
}
